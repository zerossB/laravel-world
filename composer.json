{
    "name": "zerossb/laravel-world",
    "description": "Provide countries, states, and cities relations database. (Forked from khsing/laravel-world)",
    "keywords": [
        "world",
        "country",
        "city",
        "region"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">7.3|^8.0",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.1|^11.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Khsing\\World\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Guixing Bai",
            "email": "khsing.cn@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Khsing\\World\\WorldServiceProvider"
            ]
        }
    }
}